<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="webcamregkey" value="C:#Program Files (x86)#Google#Chrome#Application#chrome.exe"/>
      <add key="devicemode" value="Serial Port"/>
      <add key="serialdevice" value="COM9"/>
      <add key="serialbaud" value="115200"/>
      <add key="restdevice" value="192.168.1.55"/>
      <add key="displaymode" value="off"/>
      <add key="serialtimeout" value="5000"/>
      
    </appSettings>
    <connectionStrings>
      <add name="prod" connectionString="YourConnectionString"/>
    </connectionStrings>
</configuration>